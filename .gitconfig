[user]
    name = Alexander Grooff
    email = alexandergrooff@gmail.com
[core]
    whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
    excludesfile = ~/.gitignore
    autocrlf = false
[color]
    ui = auto
    diff = auto
    branch = auto
    status = auto
[pull]
    rebase = false
[push]
    default = current
[init]
    defaultBranch = main
